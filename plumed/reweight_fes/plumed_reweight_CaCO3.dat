# vim:ft=plumed
UNITS LENGTH=A ENERGY=eV

dist: READ FILE=colvar.data IGNORE_TIME VALUES=dist
coord: READ FILE=colvar.data IGNORE_TIME VALUES=coord
ves: READ FILE=colvar.data IGNORE_TIME VALUES=b1.bias

weights: REWEIGHT_BIAS TEMP=300 ARG=ves.bias

HISTOGRAM ...
  ARG=dist,coord
  GRID_MIN=2,5
  GRID_MAX=12,9
  GRID_BIN=300,300
  BANDWIDTH=0.05,0.05
  LOGWEIGHTS=weights
  LABEL=hist
... HISTOGRAM

fes: CONVERT_TO_FES GRID=hist TEMP=300

DUMPGRID GRID=fes FILE=fes FMT=%24.16e
